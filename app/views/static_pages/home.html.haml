.container-fluid
    .row
        .col-md-6.offset-md-3
            = form_for :session do |f|
                .form-group
                    = f.label :email
                    = f.text_field :email, {class: "form-control"}
                .form-group
                    = f.label :password
                    = f.password_field :password, {class: "form-control"}

                -# .col-md-offset-1.col-md-4
                -# = f.submit "Submit", class: "btn btn-primary"
    .row
        .offset-md-3.col-md-6.google-sign-in
            = link_to image_tag("btn_google_signin_dark_normal_web.png", alt: "Sign In logo"), 'http://rubyonrails.org/'